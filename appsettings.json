{
  "Logging": {
    "IncludeScopes": false,
      "LogLevel": {
        "Default": "Debug",
        "System": "Information", 
        "Microsoft": "Information"
      }
    },
    "StaticFiles": {
      "Header": {
        "Cache-Control": "no-cache, no-store",
        "Pragma": "no-cache",
        "Expire": "-1"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
}
