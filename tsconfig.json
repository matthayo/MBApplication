{
    "compileOnSave": false,
    "compilerOptions": {
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "module": "system",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noEmitOnError":false,
        "removeComments": false,
        "target": "es5",
        "lib": ["es5", "es6", "dom"]
    },
    "exclude": [
        "node_modules",
        "wwwroot"
    ]   
}